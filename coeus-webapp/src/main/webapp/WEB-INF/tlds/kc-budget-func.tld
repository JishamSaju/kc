<taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd" version="2.1">

    <description>KC Budget functions library</description>
    <display-name>KC Budget functions</display-name>
    <tlib-version>1.0</tlib-version>
    <short-name>budget-kc-fn</short-name>
    <uri>http://www.kuali.org/jsp/jstl/functions</uri>
    
    <function>
        <description>Coerces the type of BudgetDecimal to a BigDecimal</description>
        <name>getBigDecimal</name>
        <function-class>org.kuali.kra.budget.web.JstlFunctions</function-class>
        <function-signature>java.math.BigDecimal getBigDecimal(org.kuali.kra.budget.BudgetDecimal)</function-signature>
        <example>&lt;c:set var="cumTotalCost" value="${cumTotalCost + krabfn:getBigDecimal(budgetPeriodObj.totalCost)}" /"&gt;</example>
    </function>
    
</taglib>
